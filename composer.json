{
  "name": "ict-corps-members-hub",
  "description": "A web application for ICT Corps members to connect and share resources.",
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0",
    "vlucas/phpdotenv": "^5.4",
    "illuminate/database": "^8.0",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "php artisan package:discover --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}